# GitHub OAuth 2.0 App Credentials
GITHUB_CLIENT_ID=your_github_oauth_client_id
GITHUB_CLIENT_SECRET=your_github_oauth_client_secret

# Session Secret (for securing cookies)
SESSION_SECRET=a_long_random_string_for_securing_sessions

# Redirect URI (callback URL registered in GitHub OAuth app)
REDIRECT_URI=http://localhost:3000/auth/github/callback

# Port for the server (optional, defaults to 3000)
PORT=3000